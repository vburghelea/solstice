{
  "name": "roundup-games",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm i18n:copy && vite build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "db": "drizzle-kit",
    "db:generate": "pnpm db generate",
    "db:migrate": "pnpm db migrate",
    "db:studio": "pnpm db studio",
    "db:push": "pnpm db push",
    "db:c15t": "drizzle-kit --config=drizzle.c15t.config.ts",
    "db:c15t:push": "pnpm db:c15t push",
    "db:c15t:studio": "pnpm db:c15t studio",
    "deps": "pnpm dlx npm-check-updates@latest --interactive --format group",
    "ui": "pnpm dlx shadcn@latest",
    "auth:secret": "pnpm dlx @better-auth/cli@latest secret",
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config ./src/lib/auth/index.ts --y --output ./src/db/schema/auth.schema.ts && prettier --write ./src/db/schema/auth.schema.ts",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "pnpm test:e2e:setup && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "tsx scripts/seed-e2e-data.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:smoke": "pnpm test:e2e:setup && pnpm exec playwright test e2e/tests/unauthenticated/auth-flow.unauth.spec.ts e2e/tests/authenticated/player-workspace.shared.spec.ts e2e/tests/authenticated/logout.shared.spec.ts e2e/tests/authenticated/profile-edit.auth.spec.ts e2e/tests/authenticated/teams-create-no-conflict.auth.spec.ts --reporter=line --project=chromium-unauthenticated --project=chromium-authenticated --workers=1",
    "prepare": "husky",
    "preinstall": "node scripts/generate-auth-secret.js",
    "docs:reference": "typedoc",
    "docs:erd": "node scripts/generate-erd.js",
    "docs:all": "pnpm docs:reference && pnpm docs:erd",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:generate-types": "node scripts/generate-translation-types.js",
    "i18n:sync": "node scripts/sync-translations.js",
    "i18n:copy": "cp -rf src/lib/i18n/locales/* public/locales/"
  },
  "dependencies": {
    "@c15t/backend": "^1.8.0",
    "@c15t/react": "^1.8.2",
    "@neondatabase/serverless": "^1.0.2",
    "@paralleldrive/cuid2": "^3.0.6",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@t3-oss/env-core": "^0.13.10",
    "@tanstack/pacer": "^0.17.2",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-pacer": "^0.19.2",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.150.0",
    "@tanstack/react-router-devtools": "^1.150.0",
    "@tanstack/react-router-ssr-query": "^1.150.0",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.150.0",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@vercel/postgres": "^0.10.0",
    "better-auth": "^1.4.13",
    "c15t": "^1.8.2",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color": "^5.0.3",
    "crawlee": "3.11.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fast-xml-parser": "^5.3.3",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fs-backend": "^2.6.1",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.562.0",
    "neon": "^2.0.0",
    "netlify-cli": "^23.13.3",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.8",
    "posthog-js": "^1.321.2",
    "posthog-node": "^5.21.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "resend": "^6.7.0",
    "sonner": "^2.0.7",
    "square": "^43.2.1",
    "tailwind-merge": "^3.4.0",
    "vite": "^7.3.1",
    "zod": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": "prettier --write"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.6.4",
    "@eslint/js": "^9.39.2",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@netlify/edge-functions": "^3.0.3",
    "@netlify/vite-plugin-tanstack-start": "^1.2.6",
    "@playwright/test": "^1.57.0",
    "@rollup/rollup-darwin-x64": "4.44.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.141.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/color": "^4.2.0",
    "@types/i18next": "^13.0.0",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "babel-plugin-react-compiler": "latest",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "fast-xml-parser": "^5.3.3",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.28.0"
}
